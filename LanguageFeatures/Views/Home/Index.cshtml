@model IEnumerable<string>

@{ Layout = null;}

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta name="viewport" content="width-device-width"/>
    <title>Language Features</title>
</head>
<body>
    <ul>
        @foreach(string s in Model)
            {
                <li>@s</li>
            }
    </ul>
</body>
</html>